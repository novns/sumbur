{% func (tags *Tags) Tags() -%}
<div class=fs-5>
{% for tags.Next() -%}
{% stripspace %}

<a href="/tag/{%z tags.tag %}" class="me-2">
    <span class="badge bg-secondary">{%z tags.tag %}
        <span class="small ms-2">{%d tags.count %}</span>
    </span>
</a>

{% endstripspace %}
{% endfor -%}
</div>
{% endfunc %}
